### Path ###
export PATH=$HOME/bin:/usr/local/bin:$PATH

### Commands ###
# history
alias h='history'

# touch
alias t='touch'

# tar
alias tzip='tar zcvf'
alias tunzip='tar xvf'

# less
alias less='less -N -S -#4'

# sublime
alias sublime_package="~/Library/Application\ Support/Sublime\ Text\ 3/Packages/User"
